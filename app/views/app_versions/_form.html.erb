<%= form_for([@project, @app_version]) do |f| %>
  <% if @app_version.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@app_version.errors.count, "error") %> prohibited this app_version from being saved:</h2>

      <ul>
      <% @app_version.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>

      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="actions">
    <%= f.label :name, "name" %>
    <%= f.text_field :name %><br  />
    <%= f.label :version, "version" %>
    <%= f.text_field :version %><br  />
    <%= f.label :notes, "notes" %>
    <%= f.text_field :notes %><br  />
    <%= f.label :app_ipa, "IPA" %>
    <%= f.file_field :app_ipa %>
    <%= f.hidden_field :url_plist, :value => "itms-services://?action=download-manifest&amp;url=http://#{request.env['HTTP_HOST']}/plist/Apartments.plist" %>
   <br  />
    <%= f.submit %>
    <%= link_to 'Back', "/projects/#{@app_version.project_id}/app_versions" %>
  </div>
<% end %>
